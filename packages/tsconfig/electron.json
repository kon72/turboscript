{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Electron",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "lib": ["esnext"],
    "module": "commonjs",
    "noEmit": false,
    "resolveJsonModule": true,
    "target": "esnext"
  }
}
